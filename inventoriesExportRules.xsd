                <xs:annotation>
                    <xs:documentation xml:lang="en">Screening done, normal results</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="DU">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Screening done, unusual findings</xs:documentation>
